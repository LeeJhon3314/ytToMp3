{
  "name": "yt-to-mp3-electron",
  "version": "1.0.0",
  "description": "YouTube to MP3 converter with Electron and Spring Boot backend",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "build": {
  "appId": "com.yt.mp3.converter",
  "productName": "YT to MP3 Converter",
  "files": [
    "**/*",
    "api/yt-to-mp3-0.0.1-SNAPSHOT.jar"
  ],
  "win": {
    "target": "nsis"
  },
  "mac": {
    "target": []
  },
  "linux": {
    "target": []
  }
},
  "devDependencies": {
  "electron": "^28.0.0",
  "electron-builder": "^24.13.3"
}

}
